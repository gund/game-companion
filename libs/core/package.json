{
  "name": "@game-companion/core",
  "version": "0.0.1",
  "description": "A core experience of any Game Companion App",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./core/src/index.d.ts",
  "exports": {
    ".": {
      "main": "./index.cjs",
      "module": "./index.js",
      "types": "./core/src/index.d.ts"
    },
    "./*": {
      "main": "./*.cjs",
      "module": "./*.js",
      "types": "./core/*/elements.d.ts"
    }
  }
}
