{
  "name": "@game-companion/mdc",
  "version": "0.0.1",
  "description": "Material Web Components based on MDC",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./mdc/src/index.d.ts",
  "exports": {
    ".": {
      "main": "./index.cjs",
      "module": "./index.js",
      "types": "./mdc/src/index.d.ts"
    },
    "./*": {
      "main": "./*.cjs",
      "module": "./*.js",
      "types": "./mdc/*/index.d.ts"
    }
  }
}
